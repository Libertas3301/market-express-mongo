<div class="row">
    <div class="col-md-8 col-md-offset-2">

        <h2 style="font-size: 60px; text-align: center;">Comezile efectuate</h2>

        {{# each orders }}
        <div class="panel panel-default">
            <div class="panel-body">
                <ul class="list-group">
                    {{# each this.items }}
                    <li class="list-group-item">
                        <span class="badge">{{ this.price }}</span>
                        {{ this.item.title }} | {{ this.qty }} Unitate
                    </li>
                    {{/each}}
                </ul>
            </div>
            <div class="panel-footer">
                <strong>Pretul total: ${{ this.cart.totalPrice }}</strong>
            </div>
        </div>
        {{/each}}
    </div>
